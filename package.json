{
  "name": "timetablemanager",
  "productName": "Kaala",
  "version": "1.0.0",
  "description": "Simple timetable manager with React and Electron",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./release-builds",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Timetable App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "axios-react": "^2.0.2",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "cross-spawn": "^7.0.3",
    "electron-reload": "^1.5.0",
    "material-table": "^1.68.0",
    "material-ui-search-bar": "^1.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.9.28",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.8.0",
    "react-easy-bar-chart": "0.0.2",
    "react-filter-search": "^1.0.11",
    "react-minimal-pie-chart": "^8.0.1",
    "react-modal": "^3.11.2",
    "react-notifications": "^1.6.0",
    "react-router-dom": "^5.2.0",
    "remote": "^0.2.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.2.0",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "reactstrap": "^8.5.1",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
